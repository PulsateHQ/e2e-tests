    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/faker": "^5.5.9",
    "@types/node": "^22.5.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^1.7.0",
    "eslint-plugin-prettier": "^5.2.1",
