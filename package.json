{
  "name": "e2e-tests",
  "version": "1.0.0",
  "description": "E2E project for CMS",
  "main": "index.js",
  "scripts": {
    "format": "npx prettier --write .",
    "format:check": "npx prettier . --check \"!**.ts\"",
    "lint": "npx eslint . --max-warnings=0",
    "test": "npx playwright test",
    "test:headed": "npm run test -- --headed",
    "test:ui": "npm run test -- --ui",
    "show-report": "npx playwright show-report",
    "test:leopard": "dotenv -e .env.leopard -- npx playwright test --ui",
    "test:leopard:headed": "dotenv -e .env.leopard -- npx playwright test --headed --project=firefox",
    "test:leopard:headless": "dotenv -e .env.leopard -- npx playwright test --project=firefox",
    "test:puma": "dotenv -e .env.puma -- npx playwright test --ui",
    "test:puma:headed": "dotenv -e .env.puma -- npx playwright test --headed --project=firefox",
    "test:puma:headless": "dotenv -e .env.puma -- npx playwright test --project=firefox",
    "test:jaguar": "dotenv -e .env.jaguar -- npx playwright test --ui",
    "test:jaguar:headed": "dotenv -e .env.jaguar -- npx playwright test --headed --project=firefox",
    "test:jaguar:headless": "dotenv -e .env.jaguar -- npx playwright test --project=firefox",
    "test:sealion": "dotenv -e .env.sealion -- npx playwright test --ui",
    "test:sealion:headed": "dotenv -e .env.sealion -- npx playwright test --headed --project=firefox",
    "test:sealion:headless": "dotenv -e .env.sealion -- npx playwright test --project=firefox",
    "test:bear": "dotenv -e .env.bear -- npx playwright test --ui",
    "test:bear:headed": "dotenv -e .env.bear -- npx playwright test --headed --project=firefox",
    "test:bear:headless": "dotenv -e .env.bear -- npx playwright test --project=firefox",
    "test:tiger": "dotenv -e .env.tiger -- npx playwright test --ui",
    "test:tiger:headed": "dotenv -e .env.tiger -- npx playwright test --headed --project=firefox",
    "test:tiger:headless": "dotenv -e .env.tiger -- npx playwright test --project=firefox",
    "test:codegen:tiger": "npx playwright codegen https://controltiger.furiousapi.com",
    "test:whale": "dotenv -e .env.whale -- npx playwright test --ui",
    "test:whale:headed": "dotenv -e .env.whale -- npx playwright test --headed --project=firefox",
    "test:whale:headless": "dotenv -e .env.whale -- npx playwright test --project=firefox",
    "test:local": "dotenv -e .env.local -- npx playwright test --ui",
